{
  "name": "@zazuko/vocabulary-extras",
  "version": "1.2.3",
  "description": "Additional vocabularies, commonly used in Zazuko projects",
  "main": "index.js",
  "type": "module",
  "bin": {
    "zazuko-vocab": "bin/vocab.js"
  },
  "exports": {
    ".": {
      "require": "./index.js",
      "import": "./index.mjs"
    },
    "./vocabularies": {
      "require": "./vocabularies.js",
      "import": "./vocabularies.mjs"
    },
    "./register": {
      "require": "./register.js",
      "import": "./register.mjs"
    },
    "./prefixes": {
      "require": "./prefixes.js",
      "import": "./prefixes.mjs"
    },
    "./datasets": {
      "require": "./datasets/index.js",
      "import": "./datasets/index.mjs"
    },
    "./builders": {
      "require": "./builders/index.js",
      "import": "./builders/index.mjs"
    },
    "./builders/loose": {
      "require": "./builders/loose.js",
      "import": "./builders/loose.mjs"
    },
    "./datasets/*": {
      "require": "./datasets/*.js",
      "import": "./datasets/*.mjs"
    }
  },
  "files": [
    "bin",
    "builders",
    "loadDataset",
    "datasets",
    "ontologies",
    "*.js",
    "*.d.ts",
    "*.d.ts.map"
  ],
  "scripts": {
    "prefetch": "mkdirp ontologies",
    "fetch": "ts-node build/fetch.ts --indexBase https://prefix.zazuko.com/",
    "lint": "eslint . --ext .js,.ts --ignore-path .gitignore --quiet",
    "pretest": "npm run lint",
    "prepare:modules": "mkdirp src/datasets; ts-node build/modules",
    "prepare:builders": "rdf-ns-builders generate -p ./src -o src/builders",
    "prebuild": "run-p prepare:*",
    "build": "tsc",
    "prepack": "npm run build",
    "release": "changeset publish"
  },
  "author": "Zazuko GmbH",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zazuko/build-your-vocabularies/issues"
  },
  "devDependencies": {
    "@changesets/cli": "^2.23.0",
    "@rdfjs/fetch-lite": "^3.2.1",
    "@rdfjs/formats-common": "^3.1.0",
    "@rdfjs/serializer-rdfjs": "^0.1.1",
    "@tpluscode/rdf-ns-builders": "^3.0.2",
    "@types/chai": "^4.2.14",
    "@types/node-fetch": "^2.5.10",
    "@types/rdf-ext": "^2.0.2",
    "@types/rdfjs__fetch-lite": "^3.0.2",
    "@types/rdfjs__formats-common": "^2.0.2",
    "@types/rdfjs__parser-n3": "^2.0.1",
    "@types/rdfjs__serializer-rdfjs": "^0.0.2",
    "@typescript-eslint/eslint-plugin": "^1.12.0",
    "@typescript-eslint/parser": "^1.12.0",
    "clownface": "^1.2.0",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^4.0.0",
    "file-fetch": "^1.6.1",
    "nodeify-fetch": "^2.2.1",
    "npm-run-all": "^4.1.5",
    "proto-fetch": "^1.0.0",
    "rdfxml-streaming-parser": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-morph": "^11.0.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@rdfjs/namespace": "^2.0.0",
    "@rdfjs/parser-n3": "^2.0.1",
    "@zazuko/prefixes": "^1.0.0-rc.0",
    "commander": "^5.0.0",
    "pkg-dir": "^5.0.0",
    "rdf-ext": "^2.2.0",
    "rdf-js": "*",
    "readable-stream": "^3.6.0",
    "string-to-stream": "^3.0.1"
  },
  "keywords": [
    "rdf",
    "rdfjs",
    "prefix",
    "namespace",
    "ontologies",
    "vocabularies"
  ]
}
