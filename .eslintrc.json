{
  "extends": [ "@tpluscode" ],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "overrides": [
    {
      "files": ["build/*"],
      "rules": {
        "no-console": "off",
        "import/no-extraneous-dependencies": "warn"
      }
    },
    {
      "files": ["packages/vocabulary-extras-builders/vocabularies/*"],
      "rules": {
        "@typescript-eslint/no-explicit-any": "off"
      }
    }
  ]
}
